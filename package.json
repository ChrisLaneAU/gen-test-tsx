{
  "name": "gen-test-tsx",
  "version": "0.0.1",
  "description": "A test generator for frontend applications, leveraging Open AI.",
  "keywords": [
    "AI test generator",
    "React unit tests",
    "Given when then"
  ],
  "license": "MIT",
  "author": "Chris Lane",
  "bin": {
    "gen-test-tsx": "dist/gen-test-tsx.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "esbuild src/main.ts --bundle --platform=node --format=cjs --target=node22 --outfile=dist/gen-test-tsx.js --banner:js='#!/usr/bin/env node' --external:fs --external:path --external:os && chmod +x dist/gen-test-tsx.js",
    "dev": "tsx src/main.ts",
    "format": "eslint . --fix && prettier . --write",
    "lint": "eslint .",
    "test": "jest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "cleye": "^1.3.4",
    "dotenv": "^16.5.0",
    "openai": "^5.5.1",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/compat": "^1.3.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "babel-jest": "^30.0.0",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.14.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@9.15.5"
}
